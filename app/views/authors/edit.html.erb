<%= form_with url: "/authors/#{@author.id}", method: :patch, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %></br>
<%= form.label :alive %>
<%= form.text_field :alive %></br>
<%= form.label :age %>
<%= form.text_field :age %></br>
<%= form.submit 'Edit Author' %>
<% end %>